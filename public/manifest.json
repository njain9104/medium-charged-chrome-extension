{
    "manifest_version": 3,
    "name": "Medium Charged",
    "description": "Table of Contents to help readers on any page!",
    "version": "1.0",
    "icons": {
      "48": "images/icon.png"
    },
    "action": {
        "default_popup": "popup/index.html"
    },
    "content_scripts":[{
      "js": ["assets/content-script.js"],
      "matches": ["https://*/*"]
    }],
    "web_accessible_resources": [{
      "matches": ["https://*/*"],
      "resources": ["assets/*"]
   }]
  }